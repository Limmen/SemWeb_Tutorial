      <owl:Class rdf:about="http://localhost:8080/id2208/tutorial/ontologies/semwebprogram#Literature"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Academic paper</rdfs:comment>
    <rdfs:label xml:lang="en">Paper</rdfs:label>
  </owl:Class>
