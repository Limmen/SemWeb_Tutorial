      <owl:Class rdf:about="http://localhost:8080/id2208/tutorial/ontologies/semwebprogram#Paper"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="http://localhost:8080/id2208/tutorial/ontologies/semwebprogram#Paper">
