  <owl:DatatypeProperty rdf:about="http://localhost:8080/id2208/tutorial/ontologies/semwebprogram#hasCourseWorkDescription">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
